[Unit]
Description=silence detector for jack audio applications
After=systemd-user-sessions.service jackd.service systemjack-init.service
Requisite=systemjack-init.service


[Service]
ExecStart=/usr/lib/systemjack/silentjack
Restart=always
RestartSec=10
LimitMEMLOCK=infinity
LimitRTPRIO=infinity
LimitRTTIME=infinity
IOSchedulingClass=realtime
CPUSchedulingPolicy=fifo
CPUSchedulingPriority=80


[Install]
WantedBy=multi-user.target
